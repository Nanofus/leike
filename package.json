  "main": "app/main.js",
  "scripts": {
    "start": "electron ./app/main.js",
    "clean": "gulp cleandist",
    "pack": "npm run clean && npm run pack:linux && npm run pack:win",
    "pack:osx": "npm run clean && electron-packager . \"Leike\" --out=dist/osx --platform=darwin --arch=all --version=0.36.1 --icon=app/img/icon.ico",
    "pack:win": "npm run clean && electron-packager . \"Leike\" --out=dist/win --platform=win32 --arch=all --version=0.36.1 --icon=app/img/icon.ico",
    "pack:linux": "npm run clean && electron-packager . \"Leike\" --out=dist/linux --platform=linux --arch=all --version=0.36.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nanofus/leike.git"
  },
  "keywords": [
    "clipboard",
    "manager"
  ],
  "bugs": {
    "url": "https://github.com/Nanofus/leike/issues"
  },
  "homepage": "https://github.com/Nanofus/leike#readme",
  "devDependencies": {
    "del": "^2.2.0",
    "electron-builder": "^2.6.0",
    "electron-packager": "^5.2.0",
    "electron-prebuilt": "*",
    "gulp": "^3.9.0",
    "gulp-coffee": "^2.3.1",
    "gulp-sass": "^2.1.1",
    "gulp-util": "^3.0.7"
  },
  "dependencies": {
    "autolinker": "^0.22.0",
    "fs-extra": "^0.26.4",
    "highlight.js": "^9.1.0",
    "vue": "^1.0.13"
  }
}
